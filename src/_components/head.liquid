<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
{% capture resource_title %}{{ title | strip_html | strip_newlines }}{% endcapture %}
<title>{% if resource_title != "Index" %}{{ resource_title | escape }} | {{ metadata.title | escape }}{% else %}{{ metadata.title | escape }}: {{ metadata.tagline | escape }}{% endif %}</title>
{% if page.next %}
<link rel="next" href="{{ page.next.relative_url }}">
{% endif %}
{% if page.previous %}
<link rel="prev" href="{{ page.previous.relative_url }}">
{% endif %}
<meta name="turbo-cache-control" content="no-preview" />
<meta name="description" content="{{ metadata.description }}" />
<link rel="preload" href="{% asset_path fonts/WOFF2/TTF/SourceSans3-Light.ttf.woff2 %}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{% asset_path fonts/WOFF2/TTF/SourceSans3-Regular.ttf.woff2 %}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{% asset_path fonts/WOFF2/TTF/SourceSerifPro-Light.ttf.woff2 %}" as="font" type="font/woff2" crossorigin>
<link rel="stylesheet" href="{% asset_path css %}" data-turbo-track="reload"/>
<link rel="manifest" href="/manifest.json">
<link rel="apple-touch-icon" sizes="180x180" href="/assets/icons/apple-touch-icon.png">
<link rel="icon" type="image/png" sizes="32x32" href="/assets/icons/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="/assets/icons/favicon-16x16.png">
<link rel="manifest" href="/assets/icons/site.webmanifest">
<link rel="mask-icon" href="/assets/icons/safari-pinned-tab.svg" color="#5bbad5">
<link rel="shortcut icon" href="/assets/icons/favicon.ico">
<meta name="msapplication-TileColor" content="#da532c">
<meta name="msapplication-config" content="/assets/icons/browserconfig.xml">
<meta name="theme-color" content="#171717">
<script src="{% asset_path js %}" data-turbo-track="reload" defer></script>
{% live_reload_dev_js %}
{% seo %}
